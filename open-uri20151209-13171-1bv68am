    <script type="text/javascript">
        quantSegs=[];
        function qc_results(result) {
            for (var i = 0; i < result.segments.length; i++) {
                quantSegs.push("qs-" + result.segments[i].id); // Modified for ADTECH DAC tags
            }
            ADTECH.config.network = ADTECH.config.network || {};
            ADTECH.config.network.kv = ADTECH.config.network.kv || {};
            ADTECH.config.network.kv.quantsegs = quantSegs;
        }
